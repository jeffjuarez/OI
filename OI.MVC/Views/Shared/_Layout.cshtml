    <main class="page-content content-wrap">
        <div class="navbar">
            <div class="navbar-inner">
                <div class="sidebar-pusher">
                    <a href="javascript:void(0);" class="waves-effect waves-button waves-classic push-sidebar">
                        <i class="fa fa-bars"></i>
                    </a>
                </div>
                <div class="logo-box">
                    <img src="/images/DrakeleftHeaderLogo.png" alt="Powered by Drake" height="60" width="160">
                </div><!-- Logo Box -->
                <div class="search-button">
                    <a href="/Account/logout" class="log-out waves-effect waves-button waves-classic">
                        <span><i class="fa fa-sign-out m-r-xs"></i></span>
                    </a>
                </div>

                <div class="topmenu-outer">
                    <div class="top-menu">

                        <ul class="nav navbar-nav navbar-left">
                            <li>
                                <a href="javascript:void(0);" class="waves-effect waves-button waves-classic sidebar-toggle"><i class="fa fa-bars"></i></a>
                            </li>
                        </ul>
                        <div style="height: 5px; font-size: 1px; width: 1px;">&nbsp;</div>
                        <a href="http://www.o-i.com/" title="Owens-Illinois."><img src="/images/logo.png" alt="Owens-Illinois." height="52" width="145"></a>
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a href="/Account/logout" class="log-out waves-effect waves-button waves-classic">
                                    <span><i class="fa fa-sign-out m-r-xs"></i>Log out</span>
                                </a>
                            </li>
                        </ul><!-- Nav -->
                    </div><!-- Top Menu -->
                </div>
            </div>
        </div><!-- Navbar -->
        <div class="page-sidebar sidebar">
            <div class="page-sidebar-inner slimscroll">

                <ul class="menu accordion-menu">

                    <li><a href="/ChartData/BasicColumn" class="waves-effect waves-button"><span class="menu-icon glyphicon glyphicon-barcode"></span><p>OI Charts</p></a></li>


                    <li><a href="/document/UploadForm" class="waves-effect waves-button"><span class="menu-icon glyphicon glyphicon-file"></span><p>OI-Form Upload</p></a></li>

                    @if(User.IsInRole("Admin")) {
                        <li><a href="/user/index" class="waves-effect waves-button"><span class="menu-icon fa fa-user"></span><p>User Management</p></a></li>
                        }

                    @if(User.IsInRole("HR") || User.IsInRole("Admin")) {
                        <li><a href="/employees/index" class="waves-effect waves-button"><span class="menu-icon fa fa-users"></span><p>Employees</p></a></li>
                        }
                    <li><a href="/Home/AboutOI" class="waves-effect waves-button"><span class="menu-icon fa fa-glass"></span><p>About O-I</p></a></li>

                </ul>
            </div><!-- Page Sidebar Inner -->
        </div><!-- Page Sidebar -->
        <div class="page-inner">
            @RenderBody()
            <div class="page-footer">
                <p class="no-s">Copyright &copy; @DateTime.Now.Year Drake International. All rights reserved.</p>
            </div>
        </div><!-- Page Inner -->
    </main><!-- Page Content -->

